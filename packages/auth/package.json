{
  "name": "@remotify/auth",
  "version": "0.1.4",
  "description": "",
  "author": "Florian Klenk <florian@remotify.place>",
  "private": true,
  "license": "Apache-2.0",
  "resolutions": {
    "**/ua-parser-js": "0.7.28"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flodev/remotify",
    "directory": "packages/auth"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run migrate:dist:latest && node src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-sequelize-models": "sequelize-auto -o './db/models' -d remotify -h localhost -u postgres -p 5432 -x $npm_config_password -e postgres -l ts --cm p --skip-tables 'knex_migrations,knex_migrations_lock'",
    "migrate:latest": "KNEX_MIGRATION_FILE_EXTENSION=ts PORT=8081 DATABASE_URL=$(grep DATABASE_URL .env | cut -d '=' -f2) knex migrate:latest",
    "migrate:rollback": "KNEX_MIGRATION_FILE_EXTENSION=ts PORT=8081 DATABASE_URL=$(grep DATABASE_URL .env | cut -d '=' -f2) knex migrate:rollback",
    "migrate:down": "KNEX_MIGRATION_FILE_EXTENSION=ts PORT=8081 DATABASE_URL=$(grep DATABASE_URL .env | cut -d '=' -f2) knex migrate:down",
    "migrate:up": "KNEX_MIGRATION_FILE_EXTENSION=ts PORT=8081 DATABASE_URL=$(grep DATABASE_URL .env | cut -d '=' -f2) knex migrate:up",
    "migrate:make": "KNEX_MIGRATION_FILE_EXTENSION=ts npx knex migrate:make",
    "migrate:dist:latest": "knex migrate:latest --knexfile ./knexfile.js",
    "tunnel": "ssh -L 192.168.178.20:8001:127.0.0.1:8001 -N 127.0.0.1"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.2",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/platform-socket.io": "^8.1.1",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/websockets": "^8.1.1",
    "@remotify/graphql": "^0.1.3",
    "@remotify/models": "^0.1.3",
    "@types/socket.io": "^3.0.2",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "graphql-tag": "^2.12.5",
    "knex": "^0.95.11",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.7.0",
    "sequelize-typescript": "^2.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "sequelize-auto": "^0.8.4",
    "socket.io-client": "^4.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
