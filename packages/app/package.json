  "scripts": {
    "start": "concurrently --kill-others \"npm:compile:watch\" \"npm:dev:server\"",
    "start:remote": "concurrently --kill-others \"npm:compile:watch\" \"npm:remote:server\"",
    "dev:server": "NODE_ENV=development parcel --no-autoinstall -p 3000 watch index.html --no-hmr public/**/*",
    "remote:server": "NODE_ENV=development parcel --no-autoinstall --host 192.168.178.20 -p 3000 watch index.html --no-hmr public/**/*",
    "compile:watch": "tsc -p . --noEmit --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest src",
    "extrude-tiles": "cp ./public/assets/tilemaps/tilemap.png ./public/assets/tilemaps/tilemap_unextruded.png; tile-extruder --tileWidth 30 --tileHeight 30 --input ./public/assets/tilemaps/tilemap.png --output ./public/assets/tilemaps/tilemap_extruded.png; mv ./public/assets/tilemaps/tilemap_extruded.png ./public/assets/tilemaps/tilemap.png"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.3.2",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@tweenjs/tween.js": "^18.6.4",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/lodash.pick": "^4.4.6",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.13",
    "@types/react-copy-to-clipboard": "^5.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "css-loader": "^5.2.6",
    "jest": "^27.1.1",
    "lerna": "^4.0.0",
    "node-sass": "^6.0.1",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "tile-extruder": "^2.1.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "resolutions": {
    "**/ua-parser-js": "0.7.28",
    "styled-components": "^5"
  }
